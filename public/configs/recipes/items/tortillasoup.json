    "recipeIngredient": [
      "2 Chicken breasts (boneless skinless)",
      "1 Onion (chopped)",
      "3 Garlic cloves (minced)",
      "1 tablespoon Olive oil",
      "3 teaspoons Chili powder",
      "1 teaspoon Dried oregano",
      "3 teaspoons Cumin powder",
      "1 can Crushed tomatoes (28oz)",
      "1 L Chicken broth",
      "1 can Corn",
      "1 can Green chili peppers (4oz)",
      "1 can Black beans (15oz; rinsed and drained)",
      "¼ cup Cilantro (chopped)",
      "Tortilla chips",
      "Monterey jack cheese (Shredded)",
      "1 Avocado",
      "[optional] Sour cream"
    ],
    "recipeInstructions": [
      {
        "@type": "HowToStep",
        "text": "Chop the chicken breasts into cubes and cook them. Put aside"
      },
      {
        "@type": "HowToStep",
        "text": "In a large pot, sauté onion in oil. Once onion is cooked add garlic, chili powder, oregano, cumin and stir."
      },
      {
        "@type": "HowToStep",
        "text": "Add crushed tomatoes, chicken broth, corn, black beans, cilantro, cooked chicken, and bring to a boil."
      },
      {
        "@type": "HowToStep",
        "text": "Turn heat down and simmer for 10-15 minutes, stirring every so often."
      },
      {
        "@type": "HowToStep",
        "text": "Serve with chips, avocado, cheese and sour cream on top."
      }
    ]
  }
