{
  "name": "rpdaley-com-v2",
  "version": "2.0.0",
  "description": "Rewrite of rpdaley.com using React, Next.js,",
  "main": "index.js",
  "scripts": {
    "dev": "export NEXT_PUBLIC_APP_VERSION=$(git rev-parse --short HEAD) && next",
    "build": "export NEXT_PUBLIC_APP_VERSION=$(git rev-parse --short HEAD) && next build",
    "start": "export NEXT_PUBLIC_APP_VERSION=$(git rev-parse --short HEAD) && next start",
    "static": "export NEXT_PUBLIC_APP_VERSION=$(git rev-parse --short HEAD) && next build && next export",
    "analyze": "export NEXT_PUBLIC_APP_VERSION=$(git rev-parse --short HEAD) && ANALYZE=true next build",
    "lint": "next lint --fix"
  },
  "author": "RP Daley <rpdaley@gmail.com>",
  "license": "MIT",
  "dependencies": {
    "@next/bundle-analyzer": "^14.0.4",
    "babel-core": "^7.0.0-bridge.0",
    "babel-plugin-styled-components": "^2.1.4",
    "next": "^14.0.4",
    "next-compose-plugins": "^2.2.1",
    "nprogress": "^0.2.0",
    "prop-types": "^15.7.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-icons": "^5.0.1",
    "react-to-print": "^2.14.15",
    "react-transition-group": "^4.4.1",
    "sharp": "^0.33.1",
    "styled-components": "^6.1.6",
    "tailwindcss": "^3.4.1",
    "typescript": "^5.3.3"
  },
  "devDependencies": {
    "@babel/core": "^7.23.7",
    "@babel/preset-env": "^7.23.8",
    "@next/eslint-plugin-next": "^14.0.3",
    "@types/node": "20.10.7",
    "@types/react": "18.2.47",
    "@typescript-eslint/eslint-plugin": "^6.18.1",
    "@typescript-eslint/parser": "^6.18.1",
    "babel-eslint": "^10.1.0",
    "eslint": "^8.56.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-next": "^14.0.4",
    "eslint-config-prettier": "^9.1.0",
    "eslint-config-wesbos": "^4.0.1",
    "eslint-plugin-html": "^7.1.0",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-jsx-a11y": "^6.8.0",
    "eslint-plugin-prettier": "^5.1.3",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "prettier": "^3.2.2"
  },
  "eslintConfig": {
    "extends": [
      "wesbos",
      "next"
    ]
  },
  "eslintIgnore": [
    "next-env.d.ts",
    "src/types.ts"
  ]
}
