{
  "name": "rpdaley-com-v2",
  "version": "2.0.0",
  "description": "Rewrite of rpdaley.com using React, Next.js,",
  "main": "index.js",
  "scripts": {
    "dev": "export NEXT_PUBLIC_APP_VERSION=$(git rev-parse --short HEAD) && NODE_ENV=development next",
    "build": "export NEXT_PUBLIC_APP_VERSION=$(git rev-parse --short HEAD) && NODE_ENV=production next build",
    "stage": "export NEXT_PUBLIC_APP_VERSION=$(git rev-parse --short HEAD) && NODE_ENV=staging next build",
    "start": "export NEXT_PUBLIC_APP_VERSION=$(git rev-parse --short HEAD) && next start",
    "analyze": "export NEXT_PUBLIC_APP_VERSION=$(git rev-parse --short HEAD) && ANALYZE=true next build",
    "lint": "next lint --fix"
  },
  "author": "RP Daley <rpdaley@gmail.com>",
  "license": "MIT",
  "dependencies": {
    "@next/bundle-analyzer": "^15.4.5",
    "babel-core": "^7.0.0-bridge.0",
    "babel-plugin-styled-components": "^2.1.4",
    "minimist-lite": "^2.3.0",
    "next": "^15.3.4",
    "next-compose-plugins": "^2.2.1",
    "nprogress": "^0.2.0",
    "prop-types": "^15.7.2",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-helmet": "^6.1.0",
    "react-icons": "^5.5.0",
    "react-to-print": "^3.1.0",
    "react-transition-group": "^4.4.1",
    "sharp": "^0.34.2",
    "styled-components": "^6.1.18",
    "tailwindcss": "^3.4.17",
    "typescript": "^5.8.3",
    "xml2js": "^0.6.2"
  },
  "devDependencies": {
    "@babel/core": "^7.27.7",
    "@babel/preset-env": "^7.28.0",
    "@next/eslint-plugin-next": "^15.4.4",
    "@types/node": "24.2.0",
    "@types/react": "18.3.12",
    "@typescript-eslint/eslint-plugin": "^7.0.0",
    "@typescript-eslint/parser": "^8.35.0",
    "babel-eslint": "^10.1.0",
    "eslint": "^8.57.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-next": "^15.3.3",
    "eslint-config-prettier": "^9.1.0",
    "eslint-config-wesbos": "^4.3.1",
    "eslint-plugin-html": "^8.1.3",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-jsx-a11y": "^6.10.0",
    "eslint-plugin-prettier": "^5.4.1",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^4.6.2",
    "prettier": "^3.6.2"
  },
  "eslintConfig": {
    "extends": [
      "wesbos",
      "next"
    ]
  },
  "eslintIgnore": [
    "next-env.d.ts",
    "src/types.ts"
  ]
}
